<h1>New members please fill out form</h1>
<hr />
<form  action="/member/form" method="post">
  <label for="name">Name:</label>
  <input type="text" name="name" required>
  <button type="submit" name="button">Add</button>
</form>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var member in Model)
    {
      <li>@member.GetMember()</li>
    }
  </ul>
}
<h3><a href="/">Return to Main Page</a></h3>

<h3><a href="/member/catalog">View our Catalog</a></h3>
